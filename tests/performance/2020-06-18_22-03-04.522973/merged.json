{
 "modules": {
  "jmeter": {
   "properties": {
    "hostname": "127.0.0.1",
    "port": 8080,
    "management_port": 8081,
    "protocol": "http",
    "input_filepath": "kitten.jpg"
   }
  }
 },
 "reporting": [
  {
   "module": "passfail"
  },
  {
   "module": "junit-xml",
   "data-source": "pass-fail"
  },
  {
   "module": "junit-xml",
   "data-source": "sample-labels"
  },
  {
   "module": "final-stats",
   "dump-csv": "${BASEDIR}/final_stats.csv"
  }
 ],
 "settings": {
  "env": {
   "BASEDIR": "."
  }
 },
 "execution": [
  {
   "executor": "jmeter",
   "scenario": {
    "script": "tests/common/teardown.jmx"
   }
  }
 ]
}
